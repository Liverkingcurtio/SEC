HOST DISCOVERY
nmap -sT -Pn IP or nmap -Pn -T4 --min-rate 5654654654684 ip -p1-10000 2>/dev/null
for i in {1..254} ;do (ping -c 1 192.168.1.$i 2>/dev/null| grep "bytes from" &) ;done
PORT VALIDATION
nc IP PORT
nmap -Pn -T4 10.50.27.84 -p80 --script http-enum.nse
nmap -Pn -T4 10.10.10.10 -p445,139 --script sbm-os-discovery.nse (WINDOWS)
TO GET KERNEL INFO
uname -a
